{
  "name": "f7-wordpress-theme",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Foursevens/f7-wordpress-theme"
  },
  "engines": {
    "node": "~12.16"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "build:memento": "MEMENTO=true gatsby build",
    "develop": "gatsby develop --host 0.0.0.0",
    "format": "prettier --write *.js src/**/*.{js,jsx}",
    "lint": "yarn lint:code && yarn lint:style",
    "lint:code": "eslint --cache --fix '**/*.{js,jsx}'",
    "lint:style": "stylelint --fix 'src/**/*.css'",
    "memento": "npx @antoinechalifour/memento",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "start:memento": "MEMENTO=true yarn develop",
    "start:memento:nl": "F7_LOCALES=nl MEMENTO=true yarn develop",
    "start:nl": "F7_LOCALES=nl yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/pro-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "classnames": "^2.2.6",
    "f7-mobility-widget": "^1.0.3",
    "gatsby": "^2.24.9",
    "gatsby-background-image": "^0.10.2",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.20",
    "gatsby-plugin-matomo": "^0.8.3",
    "gatsby-plugin-offline": "^3.2.20",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.21",
    "gatsby-source-filesystem": "^2.3.22",
    "gatsby-source-wordpress": "^3.3.25",
    "gatsby-transformer-sharp": "^2.5.11",
    "he": "^1.2.0",
    "lodash": "^4.17.19",
    "postcss-nesting": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^1.5.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-noise": "~0.3.0",
    "eslint-config-noise-browser": "~0.3.0",
    "eslint-config-noise-node": "~0.3.0",
    "eslint-config-noise-react": "~0.3.0",
    "eslint-noise-utils": "^0.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "ssh2-sftp-client": "^5.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  }
}
